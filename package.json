{
  "name": "namas-architecture",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-crop": "^11.0.10",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "concurrently": "^7.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "server": "cd server && npm start",
    "dev": "concurrently \"npm start\" \"cd server && npm run dev\"",
    "build:full": "npm install && npm run build && cd server && npm install",
    "start:production": "cd server && npm start",
    "heroku-postbuild": "npm run build:full"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  }
}
